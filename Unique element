
#include <iostream>
using namespace std;


int main() {
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    int ans=0;
    for(int i=0;i<n;i++)
    {
        ans=ans^a[i];
    }
    
    if(ans==0){
        cout << -1;
    }
    else{
        cout<<ans;
    }
    return 0;
}
